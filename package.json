    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lodash": "^4.17.21",
    "parcel": "^2.16.3",
    "postcss": "^8.5.6",
    "postcss-nesting": "^14.0.0",
    "prettier": "^3.7.4",
